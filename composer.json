{
  "name": "analogic/alert-bundle",
  "description": "Simple Symfony bundle for email alerting PHP exceptions and Javascript exceptions",
  "keywords": ["symfony", "bundle", "alert", "exception"],
  "homepage": "https://analogic.cz/",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Stanislav Humplik",
      "email": "sh@analogic.cz"
    },
    {
      "name": "The Community",
      "homepage": "https://github.com/analogic/alert-bundle"
    }
  ],
  "support": {
    "issues": "https://github.com/analogic/alert-bundle/issues"
  },
  "require": {
    "symfony/symfony": "^2.7|^3.1"
  },
  "autoload": {
    "psr-4": { "Analogic\\AlertBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.8-dev"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
